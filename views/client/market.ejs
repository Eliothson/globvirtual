<%- include ('header')-%>
	<!-- Start Banner Area -->
	<section class="banner-area organic-breadcrumb">
		<div class="container">
			<div class="breadcrumb-banner d-flex flex-wrap align-items-center justify-content-end">
				<div class="col-first">
					<h1 style="color: black;">Market place  </h1>
					<nav class="d-flex align-items-center">
						<a href="index.html">Acceuil<span class="lnr lnr-arrow-right"></span></a>
						<a href="/market">Market<span class="lnr lnr-arrow-right"></span></a>
						<a href="/">Fashon Category</a>
					</nav>
				</div>
			</div>
		</div>
	</section>

	<section class="container">
		<div class="row">
			<% produit.forEach(function (produits){%> 
				<div class="col-lg-3 col-md-4 col-sm-6">
					<div class="single-product">
						<img class="img-fluid" src="<%= produits.chemin%>" alt="">
						<div class="product-details">
							<h6><%= produits.nomProduit %></h6>
							<div class="price">
								<h6>$ <%= produits.prix %> US </h6>
							</div>
							<div class="prd-bottom">
								<a href="/singleproduct/<%= produits._id %>" class="social-info">
									<span class="lnr lnr-move"></span>
									<p class="hover-text">view more</p>
								</a>
							</div>

						</div>
					</div>
				</div>
			<%})%>
		</div>
	</section>


	<!-- Start related-product Area -->
	<section class="related-product-area section_gap">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-lg-6 text-center">
					<div class="section-title">
						<h1>Glob Virtual </h1>
					</div>
				</div>
			</div>
		</div>
	</section>
	<!-- End related-product Area -->
<%- include ('footer')-%>
